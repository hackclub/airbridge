{
  "name": "hackclub-info",
  "version": "0.2.0",
  "description": "Public-facing API for getting info from Hack Club",
  "main": "index.js",
  "scripts": {
    "dev": "PORT=5000 bun --watch src/index.js",
    "start": "bun build/index.js",
    "build": "bun build src/index.js --outdir build --target node",
    "heroku-postbuild": "bun run build",
    "fmt": "prettier \"{tests/**,src/**,}/*.js\" --write",
    "fmtCheck": "prettier \"{tests/**,src/**,}/*.js\" --check",
    "test": "bun test",
    "production-test": "bun test --test-name-pattern production",
    "basic-test": "bun test --test-name-pattern basic"
  },
  "keywords": [],
  "author": "Max Wofford <max@maxwofford.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hackclub/api2.git"
  },
  "dependencies": {
    "@bugsnag/js": "^6.5.0",
    "@bugsnag/plugin-express": "^6.5.0",
    "airtable": "^0.12.2",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "friendly-words": "^1.1.10",
    "js-yaml": "^4.1.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "dotenv": "^8.1.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  }
}
